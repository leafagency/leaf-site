{
  "name": "leaf",
  "version": "1.0.0",
  "description": "The Leaf Website",
  "private": true,
  "scripts": {
    "build:scripts": "webpack --mode=production",
    "build:styles": "node-sass --include-path scss src/assets/styles/main.scss dist/assets/styles/main.css",
    "watch:scripts": "webpack-dev-server --mode=development",
    "watch:styles": "nodemon --ignore dist/ -e scss -x \"npm run build:styles\"",
    "git:gh-pages": "gh-pages -d dist",
    "start": "run-p watch:scripts watch:styles",
    "deploy": "run-s build:scripts build:styles git:gh-pages"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "barba.js": "^1.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "flickity": "^2.1.2",
    "flickity-imagesloaded": "^2.0.0",
    "gh-pages": "^2.0.1",
    "gsap": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "micromodal": "^0.3.2",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "sass-webpack-plugin": "^1.0.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
